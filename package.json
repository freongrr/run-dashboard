{
  "name": "run-dashboard",
  "description": "Training log",
  "version": "1.0.0",
  "homepage": "https://github.com/freongrr/run-dashboard",
  "author": {
    "name": "Fabien Cortina",
    "email": "fabien.cortina@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:freongrr/run-dashboard.git"
  },
  "scripts": {
    "start": "./node_modules/.bin/brunch watch --server",
    "build": "./node_modules/.bin/brunch build --production",
    "test": "./node_modules/.bin/jest",
    "ci": "./node_modules/.bin/jest && cat ./tests_coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "c3": "0.4.18",
    "immutability-helper": "2.4.0",
    "react": "16.0.0",
    "react-bootstrap": "0.31.3",
    "react-dom": "16.0.0",
    "react-router-dom": "4.2.2",
    "when": "3.7.8"
  },
  "devDependencies": {
    "auto-reload-brunch": "2",
    "babel-brunch": "6.1",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "brunch": "2",
    "clean-css-brunch": "2",
    "copycat-brunch": "1.1.0",
    "coveralls": "3.0.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "eslint": "4.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-react": "7.4.0",
    "expect": "21.2.1",
    "flow-brunch": "0.2.4",
    "jest": "21.2.1",
    "sass-brunch": "2.10.4",
    "uglify-js-brunch": "2.10.0"
  },
  "jest": {
    "setupFiles": [
      "./src/test/webapp/setup-enzyme.js"
    ],
    "coverageDirectory": "tests_coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html"
    ]
  }
}
